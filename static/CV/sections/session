let SessionLoad = 1
let s:so_save = &g:so | let s:siso_save = &g:siso | setg so=0 siso=0 | setl so=-1 siso=-1
let v:this_session=expand("<sfile>:p")
silent only
silent tabonly
cd ~/Documents/GithubProjects/starter-hugo-academic/static/CV/sections
if expand('%') == '' && !&modified && line('$') <= 1 && getline(1) == ''
  let s:wipebuf = bufnr('%')
endif
let s:shortmess_save = &shortmess
if &shortmess =~ 'A'
  set shortmess=aoOA
else
  set shortmess=aoO
endif
badd +1 ~/Documents/GithubProjects/starter-hugo-academic/static/CV/sections/publications.bib
badd +5 ~/Documents/GithubProjects/starter-hugo-academic/static/CV/sections/talks.tex
badd +17 ~/Documents/GithubProjects/starter-hugo-academic/static/CV/sections/research-experience.tex
badd +1 ~/Documents/GithubProjects/starter-hugo-academic/static/CV/sections/work-experience.tex
badd +1 ~/Documents/GithubProjects/starter-hugo-academic/static/CV/sections/workshops.tex
badd +14 ~/Documents/GithubProjects/starter-hugo-academic/static/CV/sections/scholarships.tex
badd +8 publications.tex
badd +36 ../main.tex
badd +8 personal-statement.tex
badd +7 education.tex
badd +2 prizes.tex
badd +2 rewiever-work.tex
badd +2 teaching.tex
argglobal
%argdel
$argadd sections
edit ~/Documents/GithubProjects/starter-hugo-academic/static/CV/sections/talks.tex
let s:save_splitbelow = &splitbelow
let s:save_splitright = &splitright
set splitbelow splitright
wincmd _ | wincmd |
split
1wincmd k
wincmd w
let &splitbelow = s:save_splitbelow
let &splitright = s:save_splitright
wincmd t
let s:save_winminheight = &winminheight
let s:save_winminwidth = &winminwidth
set winminheight=0
set winheight=1
set winminwidth=0
set winwidth=1
exe '1resize ' . ((&lines * 17 + 24) / 49)
exe '2resize ' . ((&lines * 29 + 24) / 49)
argglobal
balt teaching.tex
setlocal fdm=manual
setlocal fde=0
setlocal fmr={{{,}}}
setlocal fdi=#
setlocal fdl=0
setlocal fml=1
setlocal fdn=20
setlocal fen
silent! normal! zE
let &fdl = &fdl
let s:l = 5 - ((4 * winheight(0) + 8) / 17)
if s:l < 1 | let s:l = 1 | endif
keepjumps exe s:l
normal! zt
keepjumps 5
normal! 0166|
wincmd w
argglobal
if bufexists(fnamemodify("../main.tex", ":p")) | buffer ../main.tex | else | edit ../main.tex | endif
if &buftype ==# 'terminal'
  silent file ../main.tex
endif
balt teaching.tex
setlocal fdm=manual
setlocal fde=0
setlocal fmr={{{,}}}
setlocal fdi=#
setlocal fdl=0
setlocal fml=1
setlocal fdn=20
setlocal fen
silent! normal! zE
let &fdl = &fdl
let s:l = 36 - ((22 * winheight(0) + 14) / 29)
if s:l < 1 | let s:l = 1 | endif
keepjumps exe s:l
normal! zt
keepjumps 36
normal! 021|
wincmd w
2wincmd w
exe '1resize ' . ((&lines * 17 + 24) / 49)
exe '2resize ' . ((&lines * 29 + 24) / 49)
tabnext 1
if exists('s:wipebuf') && len(win_findbuf(s:wipebuf)) == 0 && getbufvar(s:wipebuf, '&buftype') isnot# 'terminal'
  silent exe 'bwipe ' . s:wipebuf
endif
unlet! s:wipebuf
set winheight=1 winwidth=20
let &shortmess = s:shortmess_save
let &winminheight = s:save_winminheight
let &winminwidth = s:save_winminwidth
let s:sx = expand("<sfile>:p:r")."x.vim"
if filereadable(s:sx)
  exe "source " . fnameescape(s:sx)
endif
let &g:so = s:so_save | let &g:siso = s:siso_save
set hlsearch
doautoall SessionLoadPost
unlet SessionLoad
" vim: set ft=vim :
